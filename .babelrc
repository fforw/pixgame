{
    "presets": [
        "@babel/preset-react",
        "@babel/preset-env"
    ],
    "plugins": [
        ["@babel/plugin-proposal-class-properties", { "loose": true }],
        "@babel/plugin-syntax-dynamic-import"
    ],
    "env": {
        "test": {
            "plugins": [
                "babel-plugin-espower"
            ]
        },
        "production": {
            "plugins": [
                [
                    "transform-imports", {
                    "reactstrap": {
                        "transform": "reactstrap/lib/${member}",
                           "preventFullImport": true
                    }
                }
                ]
            ]
        }
    }
}

